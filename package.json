{
  "homepage": "https://iharant.github.io/rs-school-react",
  "name": "reactapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node --esm server.ts",
    "cypress:run-e2e": "cypress run --e2e",
    "cypress:coverage": "npx nyc report --reporter=text-summary",
    "cypress:open": "cypress open",
    "cypress:full": "npx nyc instrument --compact=false src instrumented && npx cypress run --e2e && npx nyc report && npx nyc report --reporter=text-summary",
    "devcheck": "eslint . && prettier --check . && vite",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "prepare": "husky install .husky",
    "dev1": "vite",
    "build1": "tsc --project tsconfig.build.json && vite build",
    "preview1": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/react18": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "axios-mock-adapter": "^1.21.4",
    "cross-env": "^7.0.3",
    "cypress": "^12.10.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3"
  }
}
